{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.8.4" } }],
          "stage-0",
          "react"
        ],
        "plugins": [
          "add-module-exports",
          "react-require",
          "transform-async-to-generator",
          "transform-es2015-classes",
          "react-hot-loader/babel",
          ["module-resolver", {
            "root": ["src", "src/lib"]
          }]
        ],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.8.4" } }],
          "stage-0",
          "react"
        ],
        "plugins": [
          "add-module-exports",
          "react-require",
          "transform-async-to-generator",
          "transform-es2015-classes",
          ["module-resolver", {
            "root": ["src", "src/lib"]
          }]
        ],
        "sourceMaps": "none"
      }
    }
  }
}
